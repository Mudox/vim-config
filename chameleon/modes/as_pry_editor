call MergeConfigs(s:sub_configs)

" }}}1

" Bundles in Current Config {{{1

let s:common_bundle = [
      \ 'neomake',
      \ ]

let s:bundle_list = s:common_bundle

call AddBundles(s:bundle_list)
" }}}1
