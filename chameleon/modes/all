" vim: filetype=vim foldmethod=marker fileformat=unix

" Set Tile & Bundle Manager {{{1

call SetBundleManager('NeoBundle')

" }}}1


let s:metas = g:chameleon.metasAvail()

if !has('unix')
    call filter(s:metas, 'v:val != "ycm"')
    call filter(s:metas, 'v:val != "autoforamt"')
endif


call AddBundles(s:metas)
