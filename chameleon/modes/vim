" Merge Configs {{{1

let s:sub_configs = [
      \ 'x_common'   ,
      \ 'x_git'      ,
      \ ]

call MergeConfigs(s:sub_configs)

" }}}1

" Bundles in Current Config {{{1

let s:bundle_list = [
      \ 'syntastic',
      \ 'vimparser',
      \ 'vimlint'  ,
      \ ]

call AddBundles(s:bundle_list)
" }}}1
