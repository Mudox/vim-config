" vim: filetype=vim foldmethod=marker fileformat=unix

" Set Tile & Bundle Manager {{{1

call SetTitle('vim mode')

call SetBundleManager('NeoBundle')

" }}}1

" Merge Configs {{{1

let s:sub_configs = [
      \ 'x_essential',
      \ 'x_common',
      \ 'x_ext_tools',
      \ 'x_color',
      \ 'x_textobj',
      \ ]

call MergeConfigs(s:sub_configs)

" }}}1

" Bundles in Current Config {{{1

let s:bundle_list = [
      \ 'rainbow_oblitum'
      \ ]

call AddBundles(s:bundle_list)
" }}}1
