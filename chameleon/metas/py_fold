" name and site
let g:this_meta.site = 'https://github.com/tmhedberg/SimpylFold.git'

" extra settings for neobundle

function g:this_meta.config() dict
     let g:SimpylFold_docstring_preview = 1
     let g:SimpylFold_fold_docstring = 1
endfunction
