" vim: filetype=vim foldmethod=marker fileformat=unix

let g:this_meta.name = expand('<sfile>:p:t')

let g:this_meta.site = 'https://github.com/mattn/emmet-vim.git'


let g:this_meta.neodict.name = g:this_meta.name


function g:this_meta.config()
  let g:user_emmet_expandabbr_key = '<M-e>'
  let g:user_emmet_next_key       = '<M-f>'
  let g:user_emmet_prev_key       = '<M-s>'

  let g:user_emmet_settings = {
        \ 'indentation' : ' '
        \ }

endfunction
