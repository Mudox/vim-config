" vim: filetype=vim foldmethod=marker fileformat=unix

let g:this_meta.site = 'https://github.com/gcmt/breeze.vim.git'

" NOTE: the dict bellow will be passed to command 'PlugInstall'
" as the 2nd argument to install this plugin.
let g:this_meta.vimplug_cmd_dict = {}

function g:this_meta.config()
    " jump to all visible opening tags after the cursor position
    nmap ]> <Plug>(breeze-jump-tag-forward)

    " jump to all visible opening tags before the cursor position
    nmap [< <Plug>(breeze-jump-tag-backward)

    " jump to all visible HTML attributes after the cursor position
    "nmap ,g> <Plug>(breeze-jump-attribute-forward)

    " jump to all visible HTML attributes before the cursor position
    "nmap ,g< <Plug>(breeze-jump-attribute-backward)

  " }}}1
endfunction
