" vim: filetype=vim foldmethod=marker fileformat=unix

let g:this_meta.site = 'https://github.com/pelodelfuego/vim-swoop.git'

" NOTE: the dict bellow will be passed to command 'PlugInstall'
" as the 2nd argument to install this plugin.
let g:this_meta.vimplug_cmd_dict = {'on': ''}

function g:this_meta.config()
  "let g:swoopUseDefaultKeyMap = 0

  "nmap ,l :call Swoop()<CR>
  "vmap ,l :call SwoopSelection()<CR>

  "nmap ,ml :call SwoopMulti()<CR>
  "vmap ,ml :call SwoopMultiSelection()<CR>

  call ctrlp#vim_plug_lazy_plugins#register(
        \   'swoop',
        \   'call plug#load("vim-swoop")'
        \ )
endfunction
