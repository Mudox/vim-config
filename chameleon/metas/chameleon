" vim: filetype=vim foldmethod=marker fileformat=unix

" name and site
let g:this_meta.name = expand('<sfile>:p:t')
let g:this_meta.site = 'git@github.com:Mudox/vim-chameleon.git'

" extra settings for neobundle
let g:this_meta.neodict.name = g:this_meta.name

function g:this_meta.config() dict
  nnoremap <silent> <Enter>B :EditMeta<Space>
  nmap <silent> <Enter>b <Plug>(Chameleon_Edit_Meta_OmniMenu)

  nnoremap <silent> <Enter>C :EditMode<Space>
  nmap <silent> <Enter>c <Plug>(Chameleon_Edit_Mode_OmniMenu)
endfunction
