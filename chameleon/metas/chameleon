" vim: filetype=vim foldmethod=marker fileformat=unix

" name and site
let g:this_meta.site = 'git@github.com:Mudox/vim-chameleon.git'

" extra settings for neobundle

function g:this_meta.config() dict
  nmap <silent> <BS>b <Plug>(Chameleon_Edit_Meta_OmniMenu)
  nmap <silent> <BS>c <Plug>(Chameleon_Edit_Mode_OmniMenu)
  nmap <silent> <Space>x <Plug>(OmniMenu_ChamNewInstance)
endfunction
