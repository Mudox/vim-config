" vim: filetype=vim foldmethod=marker fileformat=unix

" name and site
let g:this_meta.site = 'https://github.com/szw/vim-ctrlspace.git'

" extra settings for neobundle

function g:this_meta.config() dict
  let g:ctrlspace_use_tabline = 1
  let g:ctrlspace_default_mapping_key = '<Space>w'
  let g:ctrlspace_unicode_font = 0

endfunction
