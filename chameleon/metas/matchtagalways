" vim: filetype=vim foldmethod=marker fileformat=unix

" name and site
let g:this_meta.site = 'https://github.com/Valloric/MatchTagAlways.git'

" extra settings for neobundle

function g:this_meta.config() dict
  let g:mta_filetypes = {
        \ 'html'     : 1,
        \ 'xhtml'    : 1,
        \ 'xml'      : 1,
        \ 'jinja'    : 1,
        \ 'markdown' : 1,
        \ }
endfunction
