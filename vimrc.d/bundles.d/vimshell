" vim: filetype=vim foldmethod=marker fileformat=unix

" WARNING: DO NOT CHANGE   {{{1
let s:cfg_name = expand('<sfile>:p:t:r')

let s:dict = {}
let s:dict.name = s:cfg_name
" }}}1

" TODO: repository address.
let s:repo = "'Shougo/vimshell'"

" TODO: other configurations.
let s:dict.lazy = 1
let s:dict.autoload = {
            \   'commands' : [
            \       {
            \       'name'     : 'VimShell',
            \       'complete' : 'customlist,vimshell#complete'
            \       },
            \       'VimShellExecute',
            \       'VimShellInteractive',
            \       'VimShellTerminal',
            \       'VimShellPop'
            \   ],
            \   'mappings' : [ '<Plug>(vimshell_switch)' ]
            \ }


" TODO: config
function! s:cfg_func()
    nnoremap <leader><leader>s :VimProcBang<Space>
endfunction

" WARNING: DO NOT CHANGE   {{{1
execute "NeoBundle " . s:repo . ', ' . string(s:dict)
execute 'let g:CONFIG_PLUGIN_' . s:cfg_name . '= function("s:cfg_func")'
" }}}1
