let g:this_meta.name = expand('<sfile>:p:t')

let g:this_meta.site = 'https://github.com/glts/vim-cottidie.git'


let g:this_meta.neodict.name = g:this_meta.name
let g:this_meta.neodict.lazy = 1
let g:this_meta.neodict.autoload = { 'commands' : ['CottidieTip'] }

function g:this_meta.config() dict
    command! Tips CottidieTip

endfunction